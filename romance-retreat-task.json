{
  "family": "romance-retreat-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "executionRoleArn": "arn:aws:iam::606635532539:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::606635532539:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "romance-retreat",
      "image": "606635532539.dkr.ecr.ap-southeast-2.amazonaws.com/xsavlab/romance-retreat:latest",
      "essential": true,
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/romance-retreat",
          "awslogs-region": "ap-southeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "volumes": []
}
